{
    "name": "tyrellsys/cakephp3-ip-filter",
    "description": "restrict access by ip address for CakePHP4",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "wikimedia/ip-set": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Tyrellsys\\CakePHP3IpFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyrellsys\\CakePHP3IpFilter\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
